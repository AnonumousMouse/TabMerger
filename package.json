{
  "name": "tabmerger",
  "version": "2.0.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
    "react-icons": "^3.11.0",
    "react-scripts": "^4.0.2",
    "react-toastify": "^7.0.3",
    "react-tooltip": "^4.2.14",
    "reactour": "^1.18.0",
    "styled-components": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@stryker-mutator/core": "^4.4.1",
    "@stryker-mutator/jest-runner": "^4.4.1",
    "@testing-library/react": "^11.2.2",
    "@types/chrome": "0.0.133",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "jsdoc": "^3.6.6",
    "typescript": "^4.2.3",
    "zip-a-folder": "0.0.12"
  },
  "scripts": {
    "docs": "./node_modules/.bin/jsdoc -c ./config/jsdoc.config.json",
    "tsc": "tsc --project ./config/tsconfig.json",
    "start": "react-scripts start",
    "build": "set REACT_APP_PRODUCTION=true&& react-scripts build",
    "build:dev": "react-scripts build",
    "build:firefox": "node misc/adjust_manifest.js firefox && npm run build && node misc/zip.js firefox",
    "build:chrome": "node misc/adjust_manifest.js chrome && npm run build && node misc/zip.js chrome",
    "build:all": "npm run build:firefox && npm run build:chrome && git restore public/manifest.json",
    "test": "jest --coverage --collectCoverageFrom %npm_config_covFrom% --config ./config/jest.config.js -- %npm_config_path%",
    "test:all": "jest --coverage --config ./config/jest.config.js",
    "test:app": "npm run test --covFrom=./src/components/App/*.js --path=./tests/App_spec/",
    "test:group": "npm run test --covFrom=./src/components/Group/*.js --path=./tests/Group_spec/",
    "test:tab": "npm run test --covFrom=./src/components/Tab/*.js --path=./tests/Tab_spec/",
    "test:background": "npm run test --covFrom=./public/background/*.js --path=./tests/_Background_spec/",
    "test:settings": "npm run test --covFrom=./public/settings/*.js --path=./tests/_Settings_spec/",
    "mutate": "npx stryker run -m %npm_config_files% ./config/stryker.conf.js",
    "mutate:all": "npm run mutate --files=src/components/**/*_*.js,public/**/*_*.js All",
    "mutate:app": "npm run mutate --files=src/components/App/App_*.js",
    "mutate:app-help": "npm run mutate --files=src/components/App/App_helpers.js App_Help",
    "mutate:app-func": "npm run mutate --files=src/components/App/App_functions.js App_Func",
    "mutate:group": "npm run mutate --files=src/components/Group/Group_*.js Group_Func",
    "mutate:tab": "npm run mutate --files=src/components/Tab/Tab_*.js",
    "mutate:tab-help": "npm run mutate --files=src/components/Tab/Tab_helpers.js Tab_Help",
    "mutate:tab-func": "npm run mutate --files=src/components/Tab/Tab_functions.js Tab_Func",
    "mutate:background": "npm run mutate --files=public/background/background_*.js",
    "mutate:background-help": "npm run mutate --files=public/background/background_helpers.js Background_Help",
    "mutate:background-func": "npm run mutate --files=public/background/background_functions.js Background_Func",
    "mutate:settings": "npm run mutate --files=public/settings/settings_*.js",
    "mutate:settings-help": "npm run mutate --files=public/settings/settings_helpers.js Settings_Help",
    "mutate:settings-func": "npm run mutate --files=public/settings/settings_functions.js Settings_Func"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "env": {
      "webextensions": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
