{
  "name": "tabmerger",
  "version": "3.0.0",
  "description": "Stores your tabs in one location to save memory usage and increase your productivity",
  "author": "Lior Bragilevsky <lbragile.masc@gmail.com> (https://github.com/lbragile)",
  "private": true,
  "dependencies": {
    "nanoid": "^3.1.30",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
