{
  "manifest_version": 2,
  "name": "TabMerger",
  "version": "0.1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": ["tabs", "activeTab", "<all_urls>"],

  "browser_action": {
    "default_icon": "logo192.png",
    "default_title": "Sort your tabs into tab groups"
  }
}
