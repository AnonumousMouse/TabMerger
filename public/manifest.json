{
  "manifest_version": 3,
  "version": "3.0.0",
  "name": "TabMerger",
  "description": "Stores your tabs in one location to save memory usage and increase your productivity",
  "author": "Lior Bragilevsky",
  "homepage_url": "https://lbragile.github.io/TabMerger-Extension/",
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage",
    "commands",
    "contextMenus",
    "alarms",
    "downloads",
    "downloads.shelf"
  ],
  "icons": {
    "16": "images/logo16.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "action": {
    "default_icon": {
      "16": "images/logo16.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    },
    "default_popup": "index.html",
    "default_title": "Store your tabs in highly customizable containers"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+0"
      }
    },
    "currentGroup": {
      "description": "Select Current Group"
    },
    "previousGroup": {
      "description": "Select Previous Group"
    },
    "nextGroup": {
      "description": "Select Next Group"
    },
    "exclude": {
      "suggested_key": {
        "default": "Alt+X"
      },
      "description": "Filter Out Tab"
    },
    "undoAction": {
      "suggested_key": {
        "default": "Alt+U"
      },
      "description": "Toggle Undo"
    },
    "redoAction": {
      "suggested_key": {
        "default": "Alt+R"
      },
      "description": "Toggle Redo"
    },
    "import": {
      "description": "Perform Import"
    },
    "export": {
      "description": "Perform Export"
    },
    "sync": {
      "description": "Perform Sync"
    },
    "find": {
      "description": "Focus Search Bar"
    }
  },
  "incognito": "spanning"
}
